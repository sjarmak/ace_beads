{
  "name": "ace-beads-amp",
  "version": "0.1.0",
  "description": "ACE framework implementation using Amp subagents and Beads",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "ace": "dist/src/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "build:cli": "bun build src/cli.ts --compile --outfile dist/ace",
    "build:binaries": "./scripts/build-binaries.sh",
    "test": "vitest --run",
    "lint": "eslint . --ext .ts,.tsx",
    "typecheck": "tsc --noEmit",
    "dev": "tsc --watch",
    "cli": "node dist/cli.js"
  },
  "keywords": [
    "ace",
    "beads",
    "amp",
    "agentic",
    "context-engineering"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.54.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.20.2",
    "commander": "^11.1.0",
    "zod": "^3.22.0"
  }
}
